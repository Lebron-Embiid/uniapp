<view class="content" bind:__l="__l"><view class="header"><image src="../../static/shilu-login/logo.png"></image></view><view class="list"><view class="list-call"><image class="img" src="/static/1.png"></image><input class="biaoti" type="number" maxlength="11" placeholder="手机号" data-event-opts="{{[['input',[['__set_model',['$0','phoneno','$event',[]],['']]]]]}}" value="{{phoneno}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/3.png"></image><input class="biaoti" type="number" maxlength="6" placeholder="验证码" data-event-opts="{{[['input',[['__set_model',['$0','code','$event',[]],['']]]]]}}" value="{{code}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getcode',['$event']]]]]}}" class="{{['yzm',(second>0)?'yzms':'']}}" bindtap="__e">{{yanzhengma}}</view></view><view class="list-call"><image class="img" src="/static/2.png"></image><input class="biaoti" type="text" maxlength="32" placeholder="登录密码" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['']]]]]}}" value="{{password}}" bindinput="__e"/><image class="img" src="{{showPassword?'/static/shilu-login/op.png':'/static/shilu-login/cl.png'}}" data-event-opts="{{[['tap',[['display',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="dlbutton" hover-class="dlbutton-hover" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" bindtap="__e"><text>注册</text></view></view>