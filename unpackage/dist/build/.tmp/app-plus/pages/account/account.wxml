<view class="account_box data-v-9e3fee5e"><view class="page_bg data-v-9e3fee5e"></view><block wx:if="{{address==null||address==''}}"><view data-event-opts="{{[['tap',[['toAddress',['$event']]]]]}}" class="acc_item borbom notop data-v-9e3fee5e" bindtap="__e"><text class="data-v-9e3fee5e">请选择收货地址</text><view class="acc_right data-v-9e3fee5e"><image src="../../static/next.png" mode="widthFix" class="data-v-9e3fee5e"></image></view></view></block><block wx:else><view data-event-opts="{{[['tap',[['toAddress',['$event']]]]]}}" class="acc_item borbom notop default data-v-9e3fee5e" bindtap="__e"><view class="default_address data-v-9e3fee5e"><view class="oi_name data-v-9e3fee5e">{{address.name}}<text class="data-v-9e3fee5e">{{address.mobile}}</text></view><view class="oi_address data-v-9e3fee5e"><image src="../../static/car_icon2.png" mode="widthFix" class="data-v-9e3fee5e"></image>{{address.province+address.city+address.district+address.detail}}</view></view><view class="acc_right data-v-9e3fee5e"><image src="../../static/next.png" mode="widthFix" class="data-v-9e3fee5e"></image></view></view></block><view class="acc_item borbom data-v-9e3fee5e"><text class="data-v-9e3fee5e">支付方式</text><picker class="acc_right data-v-9e3fee5e" value="{{index}}" range="{{array}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input data-v-9e3fee5e">{{array[index]}}</view><image src="../../static/next.png" mode="widthFix" class="data-v-9e3fee5e"></image></picker></view><view class="acc_content borbom data-v-9e3fee5e"><block wx:for="{{accountList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="content_item data-v-9e3fee5e"><view class="ac_left data-v-9e3fee5e"><image src="{{item.goods_pic}}" mode="widthFix" class="data-v-9e3fee5e"></image></view><view class="ac_right data-v-9e3fee5e"><view class="ac_title data-v-9e3fee5e">{{item.goods_name}}</view><view class="ac_info data-v-9e3fee5e"><block wx:for="{{item.attr_list}}" wx:for-item="attr" wx:for-index="idx" wx:key="idx"><text class="data-v-9e3fee5e">{{attr.attr_group_name+": "+attr.attr_name}}</text></block></view><view class="ac_type data-v-9e3fee5e"><text class="data-v-9e3fee5e">{{"x"+item.num}}</text><text class="ac_price data-v-9e3fee5e">{{"￥"+item.level_price}}</text></view></view></view></block></view><view class="acc_item borbom data-v-9e3fee5e"><text class="data-v-9e3fee5e">运费</text><view class="pr45 data-v-9e3fee5e" style="color:#fa3930;">{{'￥'+express_price+''}}</view></view><view class="acc_mess borbom data-v-9e3fee5e"><view class="txt data-v-9e3fee5e">给买家留言</view><input type="text" placeholder="请备注" name="content" data-event-opts="{{[['input',[['getMess',['$event']]]]]}}" value="{{content}}" bindinput="__e" class="data-v-9e3fee5e"/></view><view class="fixed_account data-v-9e3fee5e"><view class="fa_left data-v-9e3fee5e">{{"总计：￥"+all}}</view><view data-event-opts="{{[['~tap',[['toSubmit',['$event']]]]]}}" class="fa_right data-v-9e3fee5e" bindtap="__e">提交</view></view></view>