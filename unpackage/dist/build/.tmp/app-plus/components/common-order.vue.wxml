<template name="5307c26c"><view class="_view data-v-37c22b74 order_box"><view class="_view data-v-37c22b74 order_item" wx:key="index" key="{{index}}" wx:for="{{orderList}}" wx:for-index="index" wx:for-item="item"><view class="_view data-v-37c22b74 order_top"><view class="_view data-v-37c22b74"><image src="../../static/clock.png" mode="widthFix" class="_image data-v-37c22b74"></image>{{item.time}}</view><text class="_text data-v-37c22b74 {{[item.status ? 'active' : '']}}">{{item.statusText}}</text></view><view class="_view data-v-37c22b74 order_info" bindtap="handleProxy" data-eventid="{{'5307c26c-0-'+index}}" data-comkey="{{$k}}"><view class="_view data-v-37c22b74 oi_left"><image src="{{item.img}}" mode="widthFix" class="_image data-v-37c22b74"></image></view><view class="_view data-v-37c22b74 oi_center"><view class="_view data-v-37c22b74 oi_title">{{item.title}}</view><view class="_view data-v-37c22b74 oi_info">{{item.info}}</view><view class="_view data-v-37c22b74 oi_type">规格：{{item.type}}</view></view><view class="_view data-v-37c22b74 oi_right"><view class="_view data-v-37c22b74 oi_price">￥{{item.price}}</view><view class="_view data-v-37c22b74 oi_num">x{{item.num}}</view></view></view><view class="_view data-v-37c22b74 order_bottom"><view class="_view data-v-37c22b74 ob_price"><view class="_view data-v-37c22b74">商品实付：<text class="_text data-v-37c22b74">￥{{item.pay}}</text></view></view><view class="_view data-v-37c22b74 ob_btn"><block wx:if="{{item.finish == false}}"><button class="_button data-v-37c22b74">取消订单</button><button bindtap="handleProxy" data-eventid="{{'5307c26c-1-'+index}}" data-comkey="{{$k}}" class="_button data-v-37c22b74">去支付</button></block><block wx:else><button class="_button data-v-37c22b74">产品详情</button></block></view></view></view></view></template>