<template name="5307c26c"><view class="_view data-v-1f27f4bc order_box"><view class="_view data-v-1f27f4bc order_item" wx:key="index" key="{{index}}" wx:for="{{orderList}}" wx:for-index="index" wx:for-item="item"><view class="_view data-v-1f27f4bc order_top"><view class="_view data-v-1f27f4bc"><image src="../../static/clock.png" mode="widthFix" class="_image data-v-1f27f4bc"></image>{{item.time}}</view><text class="_text data-v-1f27f4bc {{[item.status ? 'active' : '']}}">{{item.statusText}}</text></view><view class="_view data-v-1f27f4bc order_info" bindtap="handleProxy" wx:key="idx" key="{{idx}}" data-eventid="{{'5307c26c-0-'+index+'-'+idx}}" data-comkey="{{$k}}" wx:for="{{item.goods}}" wx:for-index="idx" wx:for-item="goods"><view class="_view data-v-1f27f4bc oi_left"><image src="{{goods.img}}" mode="widthFix" class="_image data-v-1f27f4bc"></image></view><view class="_view data-v-1f27f4bc oi_center"><view class="_view data-v-1f27f4bc oi_title">{{goods.title}}</view><view class="_view data-v-1f27f4bc oi_info">{{goods.info}}</view><view class="_view data-v-1f27f4bc oi_type">规格：{{goods.type}}</view></view><view class="_view data-v-1f27f4bc oi_right"><view class="_view data-v-1f27f4bc oi_price">￥{{goods.price}}</view><view class="_view data-v-1f27f4bc oi_num">x{{goods.num}}</view></view></view><view class="_view data-v-1f27f4bc order_bottom"><view class="_view data-v-1f27f4bc ob_price"><view class="_view data-v-1f27f4bc">商品实付：<text class="_text data-v-1f27f4bc">￥{{item.pay}}</text></view></view><view class="_view data-v-1f27f4bc ob_btn"><block wx:if="{{item.finish == 1}}"><button bindtap="handleProxy" data-eventid="{{'5307c26c-1-'+index}}" data-comkey="{{$k}}" class="_button data-v-1f27f4bc">取消订单</button><button bindtap="handleProxy" data-eventid="{{'5307c26c-2-'+index}}" data-comkey="{{$k}}" class="_button data-v-1f27f4bc">去支付</button></block><block wx:else><button class="_button data-v-1f27f4bc">产品详情</button></block></view></view></view></view></template>