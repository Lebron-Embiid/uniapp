<template name="5307c26c"><view class="_view data-v-635516ee order_box"><view class="_view data-v-635516ee order_item" wx:key="index" key="{{index}}" wx:for="{{orderList}}" wx:for-index="index" wx:for-item="item"><view class="_view data-v-635516ee order_top"><view class="_view data-v-635516ee"><image src="../../static/clock.png" mode="widthFix" class="_image data-v-635516ee"></image>{{item.time}}</view><text class="_text data-v-635516ee {{[item.status ? 'active' : '']}}">{{item.statusText}}</text></view><view class="_view data-v-635516ee order_info" bindtap="handleProxy" data-eventid="{{'5307c26c-0-'+index}}" data-comkey="{{$k}}"><view class="_view data-v-635516ee oi_left"><image src="{{item.img}}" mode="widthFix" class="_image data-v-635516ee"></image></view><view class="_view data-v-635516ee oi_center"><view class="_view data-v-635516ee oi_title">{{item.title}}</view><view class="_view data-v-635516ee oi_info">{{item.info}}</view><view class="_view data-v-635516ee oi_type">规格：{{item.type}}</view></view><view class="_view data-v-635516ee oi_right"><view class="_view data-v-635516ee oi_price">￥{{item.price}}</view><view class="_view data-v-635516ee oi_num">x{{item.num}}</view></view></view><view class="_view data-v-635516ee order_bottom"><view class="_view data-v-635516ee ob_price"><view class="_view data-v-635516ee">商品实付：<text class="_text data-v-635516ee">￥{{item.pay}}</text></view></view><view class="_view data-v-635516ee ob_btn"><block wx:if="{{item.finish == false}}"><button class="_button data-v-635516ee">取消订单</button><button bindtap="handleProxy" data-eventid="{{'5307c26c-1-'+index}}" data-comkey="{{$k}}" class="_button data-v-635516ee">去支付</button></block><block wx:else><button class="_button data-v-635516ee">产品详情</button></block></view></view></view></view></template>