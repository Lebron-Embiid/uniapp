/*每个页面公共css */
.page_bg {
  background: #f5f5f7 !important;
  min-height: 100%;
  position: fixed;
  width: 100%;
  z-index: -1;
  left: 0;
  top: 0;
}
.status_bar {
  height: var(--status-bar-height);
  width: 100%;
  background-color: #00001F;
}
.top_view {
  height: var(--status-bar-height);
  width: 100%;
  position: fixed;
  background-color: #00001F;
  top: 0;
  z-index: 999;
}
.mt44 {
  margin-top: 44px;
}
.mt130 {
  margin-top: 130rpx;
}
uni-picker .uni-picker-item {
  font-size: 28rpx;
}
.form_box {
  display: block;
  float: right;
  width: 530rpx;
  background: #fff;
  margin-top: 30rpx;
  position: relative;
  border-radius: 5px;
}
.form_box input {
    width: 80%;
    height: 70rpx;
    color: #9d9d9d;
    font-size: 22rpx;
    padding: 0 20rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.form_box button {
    width: 31rpx;
    height: 31rpx;
    position: absolute;
    right: 30rpx;
    top: 20rpx;
    padding: 0;
    margin: 0;
    background: none;
    border: 0;
}
.form_box button:after {
      border: 0;
}
.form_box button image {
      display: block;
      width: 100%;
      height: 100%;
}
.list_nav {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 20;
  background: #fff;
  color: #00001f;
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
}
.list_nav view {
    width: 50%;
    float: left;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #fff;
}
.list_nav view.active {
      border-bottom: 1rpx solid #3E3E3E;
}
.index_swiper .uni-swiper-dots {
  bottom: 30rpx;
}
.index_swiper .uni-swiper-dots .uni-swiper-dot {
    width: 29rpx;
    height: 5rpx;
    border-radius: 20rpx;
    background: rgba(255, 255, 255, 0.48);
}
.index_swiper .uni-swiper-dots .uni-swiper-dot-active {
    background: white;
}
.video_item {
  margin-bottom: 30rpx;
  border-radius: 10rpx;
  -webkit-box-shadow: 0 0 20rpx #D6D6DB;
          box-shadow: 0 0 20rpx #D6D6DB;
}
.video_item .v_poster {
    position: relative;
}
.video_item .v_poster .poster_img {
      display: block;
      width: 100%;
      height: 355rpx !important;
}
.video_item .v_poster .v_play {
      position: absolute;
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.22);
      z-index: 5;
}
.video_item .v_poster .v_play image {
        display: block;
        width: 72rpx;
        height: 72rpx !important;
        position: absolute;
        left: 50%;
        top: 50%;
        -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
                transform: translate(-50%, -50%);
}
.video_item .v_info {
    background: #fff;
    padding: 25rpx 25rpx 35rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    position: relative;
}
.video_item .v_info .head_img {
      float: left;
      margin-right: 25rpx;
}
.video_item .v_info .head_img image {
        display: block;
        width: 75rpx;
        height: 75rpx !important;
        border-radius: 50%;
}
.video_item .v_info .v_title {
      color: #00001f;
      font-size: 24rpx;
      margin: 5rpx 0 6rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      white-space: nowrap;
}
.video_item .v_info .v_title.six {
        width: 60%;
}
.video_item .v_info .v_look {
      color: #4f4f50;
      font-size: 20rpx;
}
.video_item .v_info .v_download {
      position: absolute;
      right: 30rpx;
      top: 45rpx;
      z-index: 5;
}
.video_item .v_info .v_download.last {
        right: 100rpx;
}
.video_item .v_info .v_download image {
        display: block;
        width: 40rpx;
        height: 40rpx !important;
}
uni-audio {
  display: block;
}
.audio_list {
  padding: 30rpx 20rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.audio_list .audio_item {
    background: #fff;
    margin-bottom: 20rpx;
    border: 1rpx solid #E6E6E6;
    padding: 50rpx 30rpx 40rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    border-radius: 10rpx;
}
.audio_list .audio_item .audio_img {
      position: relative;
      width: 269rpx;
      height: 269rpx !important;
      margin: 0 auto 50rpx;
}
.audio_list .audio_item .audio_img .cd_img {
        display: block;
        width: 269rpx;
        height: 269rpx !important;
}
.audio_list .audio_item .audio_img .logo_img {
        position: absolute;
        width: 89rpx;
        height: 89rpx !important;
        border-radius: 50%;
        left: 50%;
        top: 50%;
        -webkit-transform: translate(-50%, -50%);
            -ms-transform: translate(-50%, -50%);
                transform: translate(-50%, -50%);
}
.audio_list .audio_item .ai_title {
      color: #00001f;
      font-size: 24rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      white-space: nowrap;
}
.news_item {
  overflow: hidden;
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid #F0F0F0;
  margin-bottom: 10rpx;
}
.news_item .item_left {
    width: 450rpx;
    float: left;
    padding-right: 20rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.news_item .item_left .news_title {
      color: #333;
      font-size: 30rpx;
      margin: 30rpx 0 10rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      white-space: nowrap;
}
.news_item .item_left .news_info {
      color: #333;
      font-size: 22rpx;
      margin: 20rpx 0 50rpx;
      overflow: hidden;
      -o-text-overflow: ellipsis;
         text-overflow: ellipsis;
      display: -webkit-box;
      -webkit-line-clamp: 2;
      -webkit-box-orient: vertical;
      word-wrap: break-word;
      word-break: break-all;
}
.news_item .item_left .news_icon {
      color: #bbb;
      font-size: 22rpx;
}
.news_item .item_left .news_icon view {
        display: inline-block;
        margin-right: 20rpx;
}
.news_item .item_left .news_icon view image {
          display: inline-block;
          vertical-align: middle;
          width: 20rpx;
          height: 20rpx !important;
          margin: 0 5rpx 3rpx 0;
}
.news_item .item_right {
    width: 260rpx;
    float: right;
}
.news_item .item_right image {
      display: block;
      width: 260rpx;
      height: 260rpx !important;
}
.not_have {
  width: 100%;
  text-align: center;
  margin: 300rpx 0 0;
  font-size: 28rpx;
  color: #999;
}
.wxParse {
  -webkit-user-select: text;
}
.wxParse image, .wxParse .img, .wxParse image > ._img {
  display: block !important;
  max-width: 100% !important;
  margin: 0 !important;
  padding: 0 !important;
  opacity: 1 !important;
}
.contact_customer {
  position: fixed;
  right: 30rpx;
  bottom: 300rpx;
  z-index: 10;
  border-radius: 50%;
  overflow: hidden;
  -webkit-box-shadow: 0 0 20rpx #f5f5f5;
          box-shadow: 0 0 20rpx #f5f5f5;
}
.contact_customer image {
    display: block;
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
}

